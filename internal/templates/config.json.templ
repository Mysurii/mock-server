{
    "port": 3000,
    "endpoints": [
      {
        "method": "POST",
        "status": 200,
        "path": "/login",
        "jsonPath": "./mock/auth/login.json"
      },
      {
        "method": "GET",
        "status": 200,
        "path": "/users",
        "jsonPath": "./mock/users/users.json"
      },
 
      {
        "method": "POST",
        "status": 201,
        "path": "/users"
      },
      {
        "method": "GET",
        "status": 200,
        "path": "/users/{id}",
        "jsonPath": "./mock/users/user.json"
      }
    ]
}